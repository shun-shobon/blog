{
  "name": "blog",
  "version": "2.0.2",
  "description": "blog.s2n.tech",
  "private": true,
  "packageManager": "pnpm@8.9.1",
  "type": "module",
  "scripts": {
    "start": "next start",
    "gen": "run-s gen:*",
    "gen:clone": "git clone https://github.com/shun-shobon/articles.git articles",
    "gen:data": "tsx scripts/generate.ts",
    "build": "pnpm run gen && run-s build:*",
    "build:next": "next build --no-lint",
    "dev": "next dev",
    "clean": "rimraf .next articles data public/articles public/sitemap*.xml public/robots.txt",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint './src/**/*.css'",
    "lint:prettier": "prettier --check './*.config.js' './src/**/*.{js,ts,jsx,tsx,css}'",
    "lint:tsc": "tsc --noEmit",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "pnpm run lint:eslint --fix",
    "lint-fix:stylelint": "pnpm run lint:stylelint --fix",
    "lint-fix:prettier": "prettier --write './*.config.js' './src/**/*.{js,ts,jsx,tsx,css}'",
    "test": "vitest",
    "test:gen": "tsx scripts/generate-test.ts",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@acab/reset.css": "^0.8.0",
    "@js-temporal/polyfill": "^0.4.4",
    "@vercel/analytics": "^1.1.1",
    "@vercel/og": "^0.5.18",
    "classnames": "^2.3.2",
    "fast-glob": "^3.3.1",
    "feed": "^4.2.2",
    "github-slugger": "^2.0.0",
    "katex": "^0.16.9",
    "mdast-util-compact": "^4.1.1",
    "mdast-util-to-string": "^3.2.0",
    "next": "^13.5.3",
    "node-html-parser": "^6.1.10",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-syntax-highlighter": "^15.5.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.2",
    "sharp": "^0.32.6",
    "ts-array-length": "^0.1.2",
    "tsx": "^3.13.0",
    "twemoji-parser": "^14.0.0",
    "unified": "^10.1.2",
    "unist-util-remove-position": "^4.0.2",
    "unist-util-visit": "^4.1.2",
    "yaml": "^2.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "~13.5.3",
    "@next/env": "~13.5.3",
    "@shun-shobon/eslint-config-base": "^1.0.8",
    "@shun-shobon/eslint-config-next": "^1.0.4",
    "@shun-shobon/eslint-config-react": "^1.0.3",
    "@shun-shobon/eslint-config-utils": "^1.0.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@tsconfig/strictest": "^2.0.2",
    "@types/katex": "^0.16.3",
    "@types/mdast": "^3.0.13",
    "@types/node": "^18.18.5",
    "@types/nprogress": "^0.2.1",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/react-syntax-highlighter": "^15.5.8",
    "@types/sharp": "^0.31.1",
    "@types/twemoji-parser": "^13.1.2",
    "@types/unist": "^2.0.8",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "endent": "^2.1.0",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.1",
    "mdast-util-math": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "schema-dts": "^1.1.2",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended": "^13.0.0",
    "typescript": "^5.2.2",
    "unist-builder": "^3.0.1",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
