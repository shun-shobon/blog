{
  "name": "blog",
  "version": "2.0.2",
  "description": "blog.s2n.tech",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "type": "module",
  "scripts": {
    "start": "next start",
    "gen": "run-s gen:*",
    "gen:clone": "git clone https://github.com/shun-shobon/articles.git articles",
    "gen:data": "tsx scripts/generate.ts",
    "build": "pnpm run gen && run-s build:*",
    "build:next": "next build --no-lint",
    "dev": "next dev",
    "clean": "rimraf .next articles data public/articles public/sitemap*.xml public/robots.txt",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint './src/**/*.css'",
    "lint:prettier": "prettier --check './*.config.js' './src/**/*.{js,ts,jsx,tsx,css}'",
    "lint:tsc": "tsc --noEmit",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "pnpm run lint:eslint --fix",
    "lint-fix:stylelint": "pnpm run lint:stylelint --fix",
    "lint-fix:prettier": "prettier --write './*.config.js' './src/**/*.{js,ts,jsx,tsx,css}'",
    "test": "vitest",
    "test:gen": "tsx scripts/generate-test.ts",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/og": "^0.6.8",
    "classnames": "^2.5.1",
    "fast-glob": "^3.3.3",
    "feed": "^4.2.2",
    "github-slugger": "^2.0.0",
    "katex": "^0.16.22",
    "mdast-util-compact": "^4.1.1",
    "mdast-util-to-string": "^3.2.0",
    "next": "^14.2.30",
    "node-html-parser": "^6.1.13",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.2",
    "sharp": "^0.34.2",
    "temporal-polyfill": "^0.2.5",
    "the-new-css-reset": "^1.11.3",
    "ts-array-length": "^0.1.3",
    "tsx": "^4.19.4",
    "twemoji-parser": "^14.0.0",
    "unified": "^10.1.2",
    "unist-util-remove-position": "^4.0.2",
    "unist-util-visit": "^4.1.2",
    "yaml": "^2.7.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "~14.2.30",
    "@next/env": "~14.2.30",
    "@shun-shobon/eslint-config-base": "^1.0.14",
    "@shun-shobon/eslint-config-next": "^1.0.9",
    "@shun-shobon/eslint-config-react": "^1.0.4",
    "@shun-shobon/eslint-config-utils": "^1.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/katex": "^0.16.7",
    "@types/mdast": "^3.0.15",
    "@types/node": "^18.19.115",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/sharp": "^0.32.0",
    "@types/twemoji-parser": "^13.1.4",
    "@types/unist": "^2.0.11",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "endent": "^2.1.0",
    "eslint": "^8.57.1",
    "husky": "^8.0.3",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.5.2",
    "mdast-util-math": "^2.0.2",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "schema-dts": "^1.1.5",
    "stylelint": "^16.10.0",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-recommended": "^14.0.1",
    "typescript": "^5.6.3",
    "unist-builder": "^3.0.1",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.9"
  }
}
